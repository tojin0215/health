Options -MultiViews 
RewriteEngine On 
RewriteBase /

RewriteCond %{HTTP_HOST} ^(api)\.(.+)$ [NC]
RewriteRule ^(setup)/?$ http://divvy.fit.%2/%1/$1 [L,R=301,NC]

RewriteCond %{HTTP_HOST} ^(api)\.(.+)$ [NC]
RewriteRule ^(xyz)/?$ http://divvy.fit.%2/%1/web/$1 [L,R=301,NC]

RewriteCond %{HTTP_HOST} !^api\. [NC]
RewriteRule ^(xyz)/?$ /web/$1 [L,R=301,NC]